    hr {
        flex: 1;
        border: none;
        height: 2px;
        background: black;
    }

    .seat {
        font-size: 0.7rem;
    }

    .hrbtn-img {
        background-color: white !important;
        border: none;
        border-radius: 22px;
        color: #414141;
        z-index:1;
    }

    .hrbtn {
        background-color: white !important;
        border: 0.1rem solid rgb(184, 184, 184);
        border-radius: 22px;
        color: #414141;
        font-size:.8rem;

    }

    .img-btn {
        background: #000000 0% 0% no-repeat padding-box;
        opacity: 0.5;
        border-radius: 18px;
        width: fit-content;
        padding: 0 6px;
